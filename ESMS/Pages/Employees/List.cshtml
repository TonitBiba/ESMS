@model IEnumerable<ESMS.Data.Model.AspNetUsers>
@{
    ViewData["Title"] = "Lista e punëtorëve";
}

<table id="listUsers" class="table table-striped table-bordered" style="width:100%">
    <thead>
        <tr>
            <th>Emri</th>
            <th>Mbiemri</th>
            <th>Ikona</th>
            <th>Data insertimit</th>
            <th>Veprimet</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
        <tr>
            <td>@Html.DisplayFor(m => user.FirstName)</td>
            <td>@Html.DisplayFor(m => user.LastName)</td>
            <td>@Html.DisplayFor(m => user.Email)</td>
            <td>@Html.DisplayFor(m => user.PhoneNumber)</td>
            <td>@Html.DisplayFor(m => user.PostCode)</td>
        </tr>
        }
    </tbody>
</table>*@

